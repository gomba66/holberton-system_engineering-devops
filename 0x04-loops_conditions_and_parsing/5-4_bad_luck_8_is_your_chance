#!/usr/bin/env bash
# This script displays an iteration of 10 times, using if statements
ITERATOR=1
while [ $ITERATOR -le 10 ]
do
    if [ $ITERATOR -eq 4 ]
    then
	echo bad luck
    elif [ $ITERATOR -eq 8 ]
    then
	echo good luck
    else
	echo Holberton School
    fi
    let ITERATOR=ITERATOR+1
done
